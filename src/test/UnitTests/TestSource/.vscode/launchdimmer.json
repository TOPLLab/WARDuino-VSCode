    "version": "0.2.0",
    "configurations": [
        {
            "type": "WARDuinoDBG",
            "request": "launch",
            "name": "Debug WARDuino",
            "program": "${workspaceFolder}/dimmer-m5stickc.wast",
            "device": {
                "name": "Dimmer",
                "debugMode": "embedded",
                "ip": "192.168.1.239",
                "wifiCredentials": "${workspaceFolder}/.wifiHotSpot.json",
                "serialPort": "/dev/cu.usbserial-8952FFEE8B",
                "baudrate": 115200,
                "fqbn": "esp32:esp32:m5stick-c",
